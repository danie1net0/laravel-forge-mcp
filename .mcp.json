{
    "mcpServers": {
        "laravel-boost": {
            "command": "php",
            "args": [
                "artisan",
                "boost:mcp"
            ]
        },
        "forge": {
          "command": "php",
          "args": [
            "artisan",
            "mcp:start",
            "forge"
          ]
        }
    }
}
